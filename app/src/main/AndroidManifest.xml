<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simant.tourandtravel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.ReviewActivity"></activity>
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity"></activity>
        <activity android:name=".activity.ProfileActivity" />
        <activity android:name=".activity.ServiceActivity" />
        <activity
            android:name=".activity.DetailSubCategoryActivity"
            android:label="@string/title_activity_detail_sub_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.SubCategoryActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>
    </application>

</manifest>